<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Add_products</servlet-name>
        <servlet-class>admin_servlets.Add_products</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Complete_Order</servlet-name>
        <servlet-class>admin_servlets.Complete_Order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Manage_Orders</servlet-name>
        <servlet-class>admin_servlets.Manage_Orders</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Process_Order</servlet-name>
        <servlet-class>admin_servlets.Process_Order</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>feedback</servlet-name>
        <servlet-class>admin_servlets.feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendMail</servlet-name>
        <servlet-class>admin_servlets.sendMail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateCard</servlet-name>
        <servlet-class>admin_servlets.updateCard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dashboard</servlet-name>
        <servlet-class>admin_servlets.dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>orderServlet</servlet-name>
        <servlet-class>orderServlet.orderServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adminServlets</servlet-name>
        <servlet-class>adminServlet.adminServlets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logoutServlet</servlet-name>
        <servlet-class>LogoutServlet.logoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Add_products</servlet-name>
        <url-pattern>/Add_products</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Complete_Order</servlet-name>
        <url-pattern>/Complete_Order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Manage_Orders</servlet-name>
        <url-pattern>/Manage_Orders</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Process_Order</servlet-name>
        <url-pattern>/Process_Order</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>feedback</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendMail</servlet-name>
        <url-pattern>/sendMail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateCard</servlet-name>
        <url-pattern>/updateCard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>orderServlet</servlet-name>
        <url-pattern>/orderServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adminServlets</servlet-name>
        <url-pattern>/adminServlets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logoutServlet</servlet-name>
        <url-pattern>/logoutServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
